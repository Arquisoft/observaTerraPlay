# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()
GET     /country                    controllers.Application.showCountries()



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET 	/hola						controllers.HolaMundo.saluda
GET 	/country					controllers.Countries.showCountry(name: String)
GET		/countries					controllers.Countries.showCountries

# Ejemplo 
GET		/proxy						controllers.Proxy.index(url: String)
